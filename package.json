  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -c prod nest start",
    "start:dev": "dotenv -c dev nest start --watch",
    "start:debug": "dotenv -c dev nest start --watch",
    "start:prod": "dotenv -c prod node dist/main",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "dotenv -c test -- jest --watch",
    "test:cov": "dotenv -c test -- jest --coverage ./test",
    "test:debug": "dotenv -c test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -c test -- jest",
    "test:e2e:slow": "dotenv -c test -- jest --config ./test/slow.jest-e2e.json",
    "migration:create": "dotenv -c dev -- npm run typeorm -- migration:create ./migrations/migration",
    "migration:generate": "dotenv -c dev -- npm run typeorm -- migration:generate ./migrations/migration -d ./data-source.ts",
    "migration:run": "dotenv -c dev -- npm run typeorm -- migration:run -d ./data-source.ts",
    "migration:revert": "dotenv -c dev -- npm run typeorm -- migration:revert -d ./data-source.ts",
    "docker:up": "docker compose -f docker-compose.dev.yml --env-file .env.dev up -d",
    "docker:stop": "docker compose -f docker-compose.dev.yml stop",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "create:admin": "dotenv -c dev -- ts-node -r tsconfig-paths/register scripts/create-admin.ts",
    "create:test-companies": "dotenv -c dev -- ts-node -r tsconfig-paths/register scripts/create-test-companies.ts"
  },
